{
  "name": "@bryce-loskie/use",
  "version": "0.0.2",
  "packageManager": "pnpm@6.32.3",
  "description": "Opinionated vue hooks",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "funding": "https://github.com/sponsors/guygubaby",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/guygubaby/use/issues"
  },
  "homepage": "https://github.com/guygubaby/use#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/use.git"
  },
  "keywords": [
    "vue hooks"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "start": "esno src/index.ts",
    "dev": "pnpm build -- --watch",
    "build": "rimraf dist && tsup",
    "release": "bumpp --commit --push --tag",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@bryce-loskie/eslint-config": "^1.0.14",
    "@types/node": "^17.0.23",
    "eslint": "^8.12.0",
    "esno": "^0.14.1",
    "rimraf": "^3.0.2",
    "tsup": "^5.12.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.8.4",
    "vue": "^3.2.31"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@bryce-loskie/utils": "^0.1.21",
    "@vueuse/core": "^8.2.4"
  }
}
