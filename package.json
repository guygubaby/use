{
  "name": "@bryce-loskie/use",
  "version": "0.1.2",
  "packageManager": "pnpm@6.32.3",
  "description": "Opinionated vue hooks and componets",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/guygubaby",
  "homepage": "https://github.com/guygubaby/use#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/use.git"
  },
  "bugs": {
    "url": "https://github.com/guygubaby/use/issues"
  },
  "keywords": [
    "vue hooks"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "start": "esno src/index.ts",
    "dev": "pnpm build -- --watch",
    "build": "tsup",
    "release": "bumpp --commit --push --tag",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@bryce-loskie/utils": "^0.1.31",
    "@vueuse/core": "^8.5.0"
  },
  "devDependencies": {
    "@bryce-loskie/eslint-config": "^2.0.1",
    "@types/node": "^17.0.35",
    "bumpp": "^7.1.1",
    "eslint": "^8.16.0",
    "esno": "^0.16.3",
    "rimraf": "^3.0.2",
    "tsup": "^6.0.1",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vitest": "^0.12.9",
    "vue": "^3.2.36"
  }
}
